
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<div class="border backgroundWhite">
    <div class="row m-3">
        <div class="col-6">
            <h2 class="text-primary">Image List</h2>
        </div>
        <div class="col-6 text-right">
            <a asp-action="Upsert" class="btn btn-primary"><i class="fas fa-plus"></i> &nbsp; Upload New Image</a>
        </div>
    </div>

    <br />
    <br />

    <div class="row m-3">
        <div class="col-12">
            <table id="tblData" class="table table-striped table-bordered" style="width:100%">
                <thead>
                    <tr>
                        <th>Name</th>   
                        <th></th>
                    </tr>
                </thead>

            </table>
        </div>
    </div>

</div>

@section Scripts{

    <script src="~/js/WebImage.js"></script>
}

